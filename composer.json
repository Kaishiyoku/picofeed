{
  "name": "kaishiyoku/picofeed",
  "description": "Modern library to handle RSS/Atom feeds",
  "homepage": "https://github.com/kaishiyoku/picofeed",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de",
      "homepage": "https://andreas-wiedel.de"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "ext-iconv": "*",
    "ext-dom": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-SimpleXML": "*",
    "zendframework/zendxml": "1.1.0"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "4.3.0",
    "symfony/yaml": "4.2.1",
    "phpunit/phpunit": "7.5.1"
  },
  "suggest": {
    "ext-curl": "PicoFeed will use cURL if present"
  },
  "autoload": {
    "psr-0": {
      "PicoFeed": "lib/"
    }
  },
  "bin": [
    "picofeed"
  ]
}
